{
    "name": "gheb/io-bundle",
    "description": "This Bundle is a bridge to the Gheb/NeatBundle",
    "keywords": ["io", "neuroevolution", "augmenting", "topologies", "neat"],
    "homepage": "https://github.com/GregoireHebert/io-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Grégoire Hébert",
            "email": "gregoire59166@gmail.com",
            "role": "Developer"
        }
    ],

    "support": {
        "email": "gregoire59166@gmail.com",
        "issues": "https://github.com/GregoireHebert/io-bundle/issues",
        "source": "https://github.com/GregoireHebert/io-bundle"
    },

    "autoload":     {
        "psr-4": { "Gheb\\IOBundle\\": "" }
    },

    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2"
    },

    "minimum-stability": "dev",
    "prefer-stable": true,

    "config": {
        "bin-dir": "bin"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
